
<section class="section profileSection">
  <img src="<%- user.picture %>" alt="">
  <h1> <%- user.name %> </h1>
  <p> <%- user.mail %> </p>
</section>

<section class="section">
  <% for (let i = posts.length - 1, post = posts[i]; i > - 1; post = posts[--i]) { %>
    <%- include("./partials/post", { post }) %>
  <% } %>
</section>
